// ◮◮◮ GENERATED BY AURORA ◮◮◮
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Piu {
  id         String   @id @unique @default(uuid())
  userid     String
  user       Users    @relation(fields: [userid], references: [id])
  texto      String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}

model Users {
  id         String   @id @unique @default(uuid())
  name       String
  email      String
  cpf        String
  phone      String
  pius       Piu[]
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}